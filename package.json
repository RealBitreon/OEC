{
  "name": "omani-encyclopedia-competition",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:https": "node server.js",
    "dev:http": "next dev",
    "dev:turbo": "next dev --turbo",
    "generate-cert": "node generate-cert-node.js",
    "migrate": "npx tsx scripts/migrate-to-supabase.ts",
    "build": "SET NODE_OPTIONS=--max_old_space_size=8192 && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "taskkill /F /IM node.exe 2>nul & rmdir /s /q .next 2>nul & del tsconfig.tsbuildinfo 2>nul & echo Cache cleared"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.39.3",
    "@types/react-google-recaptcha": "^2.1.9",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
    "framer-motion": "^11.15.0",
    "nanoid": "^5.1.6",
    "next": "16.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "svix": "^1.84.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "node-forge": "^1.3.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
